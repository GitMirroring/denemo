<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>GlissToNothing</action>
        <after>Glissando</after>
        <_label>Gliss to Nothing</_label>
        <_tooltip>Places a glissando on the current not that ends at the height of the cursor (with nothing there).</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
